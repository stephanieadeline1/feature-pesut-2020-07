{
  "root": true,
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "settings": {
    "react": {
      "pragma": "h"
    }
  },
  "extends": [
    "eslint:recommended",
    "standard-jsx",
    "plugin:jsx-a11y/recommended",
    "plugin:prettier/recommended",
    "prettier/react"
  ],
  "rules": {
    "no-console": "off"
  }
}
